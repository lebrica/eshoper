{% extends 'form_div_layout.html.twig' %}
{% block form_errors %}
    <div >
        {%- if errors|length > 0 -%}
            {%- for error in errors -%}
                <div class='alert alert-danger' id={{ id ~ "_error_msg_id" }}>{{ error.message }}</div>
            {%- endfor -%}
        {%- endif -%}
    </div>
{% endblock %}